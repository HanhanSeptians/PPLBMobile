<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".detailPengeluaran">
    <TextView
        android:id="@+id/textView"
        android:layout_width="335dp"
        android:layout_height="40dp"
        android:layout_marginTop="124dp"
        android:background="@android:drawable/editbox_background"
        android:backgroundTint="#BFBBB8"
        android:gravity="center"
        android:text="Detail Pengeluaran"
        android:textColor="#2196F3"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="335dp"
        android:layout_height="250dp"
        android:background="@android:drawable/editbox_background"
        android:backgroundTint="#FAF5F2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="40dp"
        android:text="Tanggal   :"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="30dp"
        android:text="Nominal :"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="30dp"
        android:text="Tujuan   :"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />

    <Button
        android:id="@+id/backDetailPengeluaran"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="30dp"
        android:backgroundTint="#2196F3"
        android:text="back"
        android:onClick="backs"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <ImageButton
        android:id="@+id/plusDetailPengeluaran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="40dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34"
        app:srcCompat="@drawable/plus" />

    <TextView
        android:id="@+id/tanggalDetailPengeluaran"
        android:layout_width="225dp"
        android:layout_height="40dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="32dp"
        android:background="@android:drawable/editbox_background"
        android:gravity="center|start"
        android:hint="  Detail Tanggal"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView35"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/nominalDetailPengeluaran"
        android:layout_width="225dp"
        android:layout_height="40dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:background="@android:drawable/editbox_background"
        android:gravity="center|start"
        android:hint="  Detail Nominal"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView36"
        app:layout_constraintTop_toBottomOf="@+id/tanggalDetailPengeluaran" />

    <TextView
        android:id="@+id/tujuanDetailPengeluaran"
        android:layout_width="225dp"
        android:layout_height="40dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="30dp"
        android:background="@android:drawable/editbox_background"
        android:gravity="center|start"
        android:hint="  Detail Tujuan"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView37"
        app:layout_constraintTop_toBottomOf="@+id/nominalDetailPengeluaran" />

</androidx.constraintlayout.widget.ConstraintLayout>